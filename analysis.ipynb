   "outputs": [],
   "source": [
    "#defining all constants\n",
    "m_e = 9.10938291e-31\n",
    "m_p = 1.672621777e-27\n",
    "m_n = 1.674927351e-27\n",
    "e = 1.602176565e-19\n",
    "u = 1.660538921e-27\n",
    "h = 6.62606957e-34\n",
    "hbar = 1.054571726e-34\n",
    "E0 = 8.854187817e-12\n",
    "c = 2.99792458e8\n",
    "m_He0 = 4.002603254*u\n",
    "m_H = 1.00784*u\n",
    "m_He = 4.002602*u\n",
    "m_D = 2.014*u"
   ]
